<h1>Matches</h1>

<div class="container-results">
  <ul>
  <div class="container-card">
  <% @filter_matches.each do |user_game| %>
  <div class="card-profil">
    <li><%= user_game.user.nickname %></li>
    <br>
    <div class="img-profil">
    <li><% if user_game.user.photo.attached? %>
        <%= cl_image_tag user_game.user.photo.key %>
      <%else %>
        <%= image_tag "https://res.cloudinary.com/dcd4fkkhk/image/upload/v1678372380/kisspng-emmanuel-macron-france-selfie-5b1fe5794ebb60.4981385915288170173225_qfdmzt.png"%>
      <% end %></li>
    </div>
    <br>
    <li><%= user_game.user.birthday %></li>
    <br>
    <li><%= user_game.user.city %></li>
    <br>
    <li><%= user_game.user.bio %>Bio here:</li>
    <br>
    <%= simple_form_for [user_game, @friendship] do |f| %>
      <%= f.button :submit %>
    <% end %>
  </div>
  <% end %>
  </div>
  </ul>
</div>
