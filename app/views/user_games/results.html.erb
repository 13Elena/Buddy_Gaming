 <h1>Matches</h1>
<ul>
  <% @matches.each do |card| %>
  <li><%= card.user.nickname %></li>
  <li><%= card.user.birthday %></li>
  <li><%= card.user.city %></li>
  <li><%= card.user.bio %>Bio here:</li>
  <li><%= card.user.availability %></li> <br>
  <%= link_to "Like", user_game_friendships_path(card.id), data: { turbo_method: :post } %>
  <% end %>
</ul>
